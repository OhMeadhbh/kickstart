{
  "publicUrl": "http://angelina.l.nodify.us/",
  "errorHandler": {
    "dumpExceptions": true,
    "showStack": true
  },
  "faviconPath": "static/favicon.ico",
  "logPath": "access_log.csv",
  "logFormat": "\":date\",\":req[x-forwarded-for]\",:status,:method,\":url\",\":referrer\",\":user-agent\"",
  "staticPath": "static",
  "views": {
    "path": "views",
    "engine": "hbs"
  },
  "listen": {
    "port": 9200,
    "host": "127.0.0.1"
  },
  "twitter": {
    "consumer_key": "",
    "consumer_secret": "",
    "request_token_url": "https://api.twitter.com/oauth/request_token",
    "authorize_url": "https://api.twitter.com/oauth/authorize",
    "access_token_url": "https://api.twitter.com/oauth/access_token",
    "version": "1.0A",
    "callback_url": "http://angelina.app.nodify.us/api/callback.twitter",
    "mac_algo": "HMAC-SHA1"
  },
  "email": {
    "server": {"host":"127.0.0.1"},
    "headers": {"from": "Do Not Reply <do-not-reply@angelina.app.nodify.us>"}
  }
}
